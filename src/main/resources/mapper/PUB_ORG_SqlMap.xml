         <result column="JGQC" property="jgqc" jdbcType="VARCHAR" />
         <result column="PY" property="py" jdbcType="VARCHAR" />
         <result column="FDNCODE" property="fdncode" jdbcType="VARCHAR" />
         <result column="BBXH" property="bbxh" jdbcType="VARCHAR" />
